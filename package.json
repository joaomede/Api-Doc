{
  "name": "api-doc",
  "version": "1.0.22",
  "description": "A crud for api documentation",
  "author": "Jo√£o Medeiros N. Jr - <symbol2studio@gmail.com> (https://symbol2.com.br/)",
  "homepage": "http://github.com/joaomede/Api-Doc#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+http://github.com/joaomede/Api-Doc.git"
  },
  "keywords": [
    "documentation",
    "api",
    "express",
    "typescript",
    "vuejs",
    "react",
    "crud"
  ],
  "private": true,
  "scripts": {
    "build": "rimraf dist/ && sucrase ./api -d ./dist --transforms typescript,imports",
    "dev": "nodemon api/Server.ts",
    "start:dev": "sucrase-node api/Server.ts",
    "migrate": "sucrase-node api/db/migrate",
    "migrate:down": "sucrase-node api/db/migratedown",
    "build:view": "cd view && yarn quasar build -m spa",
    "start": "node dist/Server.js",
    "version": "git add -A ./",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@hapi/joi": "^16.1.8",
    "bcrypt": "^3.0.7",
    "connect-history-api-fallback": "^1.6.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.4",
    "knex-populate": "^2.1.2",
    "nodemailer": "^6.4.2",
    "nodemailer-express-handlebars": "^3.1.0",
    "pg": "^7.15.1",
    "resp-express": "^1.1.1",
    "sqlite3": "^4.1.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/connect-history-api-fallback": "^1.3.3",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/hapi__joi": "^16.0.4",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/node": "^12.12.14",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "nodemon": "^2.0.1",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "sucrase": "^3.10.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.3"
  }
