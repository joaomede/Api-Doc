    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "jsonwebtoken": "^9.0.0",
    "nodemailer": "^6.7.2",
    "resp-express": "^2.0.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/connect-history-api-fallback": "^1.3.3",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/hapi__joi": "^16.0.4",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/node": "^12.12.14",
    "@types/nodemailer": "^6.4.0",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
